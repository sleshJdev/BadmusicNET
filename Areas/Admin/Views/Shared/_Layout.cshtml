<!DOCTYPE html>
<html>
<head runat="server">
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.css")" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="container">
        <div class="row-fluid">
            <div class="masthead">
                <h3 class="muted">
                    <i><strong>MoozZLO</strong></i></h3>
                <div class="navbar" style="margin-top: 10px">
                    <div class="navbar-inner">
                        <div class="container">
                            <ul class="nav">
                                <li>@Html.ActionLink("Главгая", "Index")</li>
                                <li>@Html.ActionLink("Пользователи", "GetAllUsers")</li>
                                <li>@Html.ActionLink("Создать пользователя", "CreateUser")</li>
                                <li>@Html.ActionLink("Контент", "GetAllTracks")</li>
                                <li>@Html.ActionLink("Дабавить трек", "Create")</li>                         
                            </ul>
                            <div class="pull-right" style="margin-top: 10px">
                                @{Html.RenderPartial("_LogOnPartial");}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row-fluid">
            @RenderBody()
        </div>
    </div>
</body>
</html>
